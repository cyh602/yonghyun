<div class="container">
<form name="achvForm" action="/evnt_mng" method="post">
  
  <!-- title -->
  <div class="bs-callout bs-callout-danger" id=callout-helper-context-color-specificity> 
    
          <% if(data2==null) { %>
            <p><h4>골드번호 이벤트</h4>응모현황 조회</p>
          <% } else if(data==null) { %>
            <p><h4>골드번호 이벤트</h4>당첨결과 확인하기</p>
          <% } %>
  </div>
  <p></p>
  
  <!-- body -->
  <div class="panel panel-default">
          <div class="panel-body">

            <% if(data!=null) { %>
                  <% data.forEach(function(e){ %>
                  
                    <div>
                      <div class="well">
                        <h4 class="text-primary">
                          <%= e._id.gold_svc_num.substr(0,3) %> - <%= e._id.gold_svc_num.substr(3,4) %> - <%= e._id.gold_svc_num.substr(7) %>
                        </h4>
                        <h4>
                          현재 <font class="text-danger"><%= e.count %></font>명 응모
                        </h4>
                      </div>
                      
                    </div>
                    
                  <% })  %>
                  
                  <p align="center"><a class="btn btn-primary" href="/num_mng/goldList" >당첨결과 확인하기</a></p>
            <% } %>
          </div>
  </div>
        
